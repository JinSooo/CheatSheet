        {
          "command": { "win": "Ctrl+L", "mac": "Command+L", "linux": "Ctrl+L" },
          "description": "跳转到地址栏"
        },
        {
          "command": { "win": "Alt+D", "mac": "Option+D", "linux": "Alt+D" },
          "description": "跳转到地址栏"
        },
        {
          "command": { "win": "F6", "mac": "F6", "linux": "F6" },
          "description": "跳转到地址栏"
        },
        {
          "command": { "win": "Ctrl+K", "mac": "Command+K", "linux": "Ctrl+K" },
          "description": "从页面中的任意位置搜索"
        },
        {
          "command": { "win": "Ctrl+E", "mac": "Command+E", "linux": "Ctrl+E" },
          "description": "从页面中的任意位置搜索"
        }
      ]
