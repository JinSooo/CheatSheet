        {
          "command": {
            "win": "Ctrl+Shift+W",
            "mac": "Command+Shift+W",
            "linux": "Ctrl+Shift+W"
          },
          "description": "关闭当前窗口"
        },
        {
          "command": { "win": "Alt+F4", "mac": "Option+F4", "linux": "Alt+F4" },
          "description": "关闭当前窗口"
