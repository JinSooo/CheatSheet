{
  "$schema": "./template/shortcut.schema.json",
  "name": "Google Chrome",
  "categories": [
    {
      "name": "标签页",
      "shortcuts": [
        {
          "command": { "win": "Ctrl+N", "mac": "Command+N", "linux": "Ctrl+N" },
          "description": "打开新窗口"
        },
        {
          "command": {
            "win": "Ctrl+Shift+N",
            "mac": "Command+Shift+N",
            "linux": "Ctrl+Shift+N"
          },
          "description": "在无痕模式下打开新窗口"
        },
        {
          "command": { "win": "Ctrl+T", "mac": "Command+T", "linux": "Ctrl+T" },
          "description": "打开新的标签页，并跳转到该标签页"
        },
        {
          "command": {
            "win": "Ctrl+Shift+T",
            "mac": "Command+Shift+T",
            "linux": "Ctrl+Shift+T"
          },
          "description": "按标签页的关闭顺序重新打开先前关闭的标签页"
        },
        {
          "command": {
            "win": "Ctrl+Tab",
            "mac": "Command+Tab",
            "linux": "Ctrl+Tab"
          },
          "description": "跳转到下一个打开的标签页"
        },
        {
          "command": {
            "win": "Ctrl+PgDn",
            "mac": "Command+PgDn",
            "linux": "Ctrl+PgDn"
          },
          "description": "跳转到下一个打开的标签页"
        },
        {
          "command": {
            "win": "Ctrl+Shift+Tab",
            "mac": "Command+Shift+Tab",
            "linux": "Ctrl+Shift+Tab"
          },
          "description": "跳转到上一个打开的标签页"
        },
        {
          "command": {
            "win": "Ctrl+PgUp",
            "mac": "Command+PgUp",
            "linux": "Ctrl+PgUp"
          },
          "description": "跳转到上一个打开的标签页"
        },
        {
          "command": {
            "win": "Alt+Home",
            "mac": "Option+Home",
            "linux": "Alt+Home"
          },
          "description": "在当前标签页中打开主页"
        },
        {
          "command": {
            "win": "Alt+Left",
            "mac": "Option+Left",
            "linux": "Alt+Left"
          },
          "description": "打开当前标签页浏览记录中记录的上一个页面"
        },
        {
          "command": {
            "win": "Alt+Right",
            "mac": "Option+Right",
            "linux": "Alt+Right"
          },
          "description": "打开当前标签页浏览记录中记录的下一个页面"
        },
        {
          "command": { "win": "Ctrl+W", "mac": "Command+W", "linux": "Ctrl+W" },
          "description": "关闭当前标签页"
        },
        {
          "command": {
            "win": "Ctrl+F4",
            "mac": "Command+F4",
            "linux": "Ctrl+F4"
          },
          "description": "关闭当前标签页"
